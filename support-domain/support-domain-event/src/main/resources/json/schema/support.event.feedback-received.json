{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/support/event/support.event.feedback-received.json",
  "type": "object",
  "properties": {
    "feedbackId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/data-types.json#/definitions/uuid"
    },
    "comment": {
      "type": "string",
      "maxLength": 1200
    },
    "refUrl": {
      "type": "string"
    },
    "refService": {
      "type": "string"
    },
    "rating": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5
    },
    "caseId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/data-types.json#/definitions/uuid"
    }
  },
  "additionalProperties": false,
  "required": [
    "feedbackId",
    "rating"
  ]
}
